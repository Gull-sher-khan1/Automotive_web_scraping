{
    "pmmonline.co.uk": {
            "allow": ["/news/page/"],
            "startPath": ["/category/news"],
            "format": {
                "site_link": "li[itemscope] [itemprop='headline'] a::attr(href)",
                "heading": "li[itemscope] [itemprop='headline'] a::text",
                "body": "li[itemscope] div.entry-content p::text",
                "image_link": "li[itemscope] figure.post-thumbnail img::attr(src)",
                "publish_date": "li[itemscope] time[itemprop='datePublished']::attr(datetime)"
            }
        }
}